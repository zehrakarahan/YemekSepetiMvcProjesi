<script type="text/javascript">

    $('#search').keyup(function () {


        $('#parent').html('');
        $('#parent').html('');
        $('#parent').html('');

        var x = document.getElementById("search").value;
        document.getElementById("demo").innerHTML = "You wrote: " + x;
        alert("merhaba dunya");

        var txt = $('#search').val().toLowerCase();
        if (txt.length > 2) {
            $.ajax({
                url: '/Anasayfalar/SeachArama/',
                data: { searchvalue: txt },
                type: 'POST',
                dataType: 'json',
                success: function (data) {
                    var veriler = "";
                    var i = "";
                    var a = "";
                    var b = "";
                    //sonuc burda lıstelenıyor
                    for (i in data.FirmaListesi) {

                        $('#parent').append('<p>' + data.FirmaListesi[i].CompanyName + '</p>');
                    }
                    for (a in data.ProductListe) {

                        $('#parent2').append('<p>' + data.ProductListe[a].Pname + '</p>');
                    }
                    for (b in data.CategoryListe) {
                        $('#parent3').append('<p>' + data.CategoryListe[b].Cname + '</p>');
                    }
                    deneme();
                }
            });
        }
        if (txt.length==0 || txt.length<2) {
            $('#parent').html('');
            $('#parent').html('');
            $('#parent').html('');
        }
        function deneme()
        {
            alert("merhabalar");
        }




    });

</script>
